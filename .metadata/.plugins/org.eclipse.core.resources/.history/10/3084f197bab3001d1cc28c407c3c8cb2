import java.util.Scanner;

public class main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int numero;
		Scanner sc = new Scanner(System.in);
		
		
		do {
			System.out.println("Qual o numero?");
			numero = sc.nextInt();
		} while (numero < 0 || numero > 20)
		
		
	}

}
