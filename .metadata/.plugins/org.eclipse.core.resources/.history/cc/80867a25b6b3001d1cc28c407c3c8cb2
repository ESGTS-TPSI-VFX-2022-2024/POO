import java.util.Scanner;

public class main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int[] v1 = new int[50];
		
		Scanner sc = new Scanner(System.in);
		
		for (int i = 0; i < v1.length; i++) {
			System.out.println("Qual o numero?");
			v1[i] = sc.nextInt();
		}
		
	}

}
